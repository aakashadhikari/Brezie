<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ascentbrezie.brezie" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.ascentbrezie.brezie.activities.AboutUsActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.ascentbrezie.brezie.activities.LandingActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.ascentbrezie.brezie.activities.LoginActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".activities.LoginOrRegisterActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.ascentbrezie.brezie.activities.DeleteActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.ascentbrezie.brezie.activities.MoodDetailActivity"
            android:label="@string/app_name"
            />
        <activity
            android:name="com.ascentbrezie.brezie.activities.ProfileActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.ascentbrezie.brezie.activities.RegisterActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.ascentbrezie.brezie.activities.SettingsActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.ascentbrezie.brezie.activities.SplashScreenActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>

